{
    "presets": [
        "stage-3",
        ["env", {
            "targets": {
                "browsers": [
                    "last 2 versions"
                ]
            },
            "modules": false
        }]
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true
        }],
        "external-helpers"
    ]
}